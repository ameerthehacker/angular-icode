{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "angluar-icode"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "../express-icode/public",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "ic",
      "styles": [
        "../node_modules/codemirror/lib/codemirror.css",                   
        "styles.scss"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/semantic-ui-sass/js/transition.js",        
        "../node_modules/semantic-ui-sass/js/dropdown.js",    
        "../node_modules/semantic-ui-sass/js/sidebar.js",   
        "../node_modules/semantic-ui-sass/js/tab.js",   
        "../node_modules/semantic-ui-sass/js/dimmer.js",                
        "../node_modules/semantic-ui-sass/js/modal.js",                    
        "../node_modules/codemirror/lib/codemirror.js", 
        "../node_modules/codemirror/mode/python/python.js", 
        "../node_modules/codemirror/mode/clike/clike.js",   
        "../node_modules/codemirror/mode/php/php.js",  
        "../node_modules/codemirror/mode/shell/shell.js",
        "scripts.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {
    }
  }
}
